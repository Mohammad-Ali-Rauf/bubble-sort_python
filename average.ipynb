    "data = [32, 15, 45, 78, 1, 12, 18, 98, 75, 55]\n",
    "\n",
    "def takeAverage(array):\n",
    "    sum = 0\n",
    "    for i in array:\n",
    "        sum = sum + i\n",
    "    return sum//len(array)\n",
    "\n",
    "average = takeAverage(data)\n",
    "\n",
    "print(average)"
   ]
